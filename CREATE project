def information_research():
    print("Loading...")
    time.sleep(2)
    print(Fastfood)
    print("Please choose one subject from the preceeding list! ")
    choice = input("What specific fast food information  you are interested in: ")
    if choice in Fastfood: 
        x = Fastfood.index(choice)
        print("We do have some information about this speciifc fast food:")
        print("""1. Calories    
2. Fats
3. Protein
4. Sodium
5. All of above""")
        information = int(input("Which of the following you are interesting in(1~5): "))
        if information == 1:
            print("The calories for "+ Fastfood[x] + " is " + str(Calories[x]) + " Calories")
        if information == 2:
            print("The fats for "+ Fastfood[x] + " is " + str(Fats[x]) + " fats")
        if information == 3:
            print("The protein for "+ Fastfood[x] + " is " + str(Protein[x]) + " proteins")
        if information == 4:
            print("The sodium for "+ Fastfood[x] + " is " + str(sodium[x]) + " sodiums")
        if information == 5:
            print("Organizing the informations we found in our datasets...")
            time.sleep(2)
            print("The calories for "+ Fastfood[x] + " is " + str(Calories[x]) + " Calories")
            print("The fats for "+ Fastfood[x] + " is " + str(Fats[x]) + " fats")
            print("The protein for "+ Fastfood[x] + " is " + str(Protein[x]) + " proteins")
            print("The calories for "+ Fastfood[x] + " is " + str(Calories[x]) + " sodiums") 
    else:      
        print("Unfortunately, we don't have information about this fast food")

def favor_selection():
    print("Are you struggling with finding your meal? Enter some requirement and we will help you to figure it out!")
    yes_no = input("Do you have any limitation on calories, fats, proteins, and sodiums: ")
    if yes_no == "y" or yes_no == "yes":
        print("Due to the immaturity of our system, we only have the following informations:")
        print("""1. Calories 
2. Fats
3. Protein
4. Sodium
5. All of above""")
        requirement = int(input("Which nutrient do you want to keep under control(Please insert number 1~5): "))
        if requirement == 1:
            calories_limit = input("What is your expected limitation on calories: ")
            for i in range(len(Calories)):  
                if str(Calories[i]) <= calories_limit:
                    filter_list.append(Fastfood[i])
            if len(filter_list) > 0:
                print("Searching...")
                time.sleep(2)
                print(filter_list)
                print("These might be some good choices for you!")
            else: 
                print("Sorry, we didn't find any fast foods satisfy your requirement")

        if requirement == 2:
            fats_limit = input("What is your expected limitation on fats: ")
            for i in range(len(Fats)):
                if str(Fats[i]) <= fats_limit:
                    filter_list.append(Fastfood[i])        
            if len(filter_list) > 0:
                print("Searching...")
                time.sleep(2)
                print(filter_list)
                print("These might be some good choices for you!")
            else: 
                print("Sorry, we didn't find any fast foods satisfy your requirement")

        if requirement == 3:
            protein_limit = input("What is your expected limitation on protein: ")
            for i in range(len(Protein)):
                if str(Protein[i]) <= protein_limit:
                    filter_list.append(Fastfood[i])
            if len(filter_list) > 0:
                print("Searching...")
                time.sleep(2)
                print(filter_list)
                print("These might be some good choices for you!")
            else: 
                print("Sorry, we didn't find any fast foods satisfy your requirement")

        if requirement == 4:
            sodium_limit = input("What is your expected limitation on sodium: ")
            for i in range(len(sodium)):
                if str(sodium [i]) <= sodium_limit:
                    filter_list.append(Fastfood[i])
            if len(filter_list) > 0:
                print("Searching...")
                time.sleep(2)
                print(filter_list)
                print("These might be some good choices for you!")
            else: 
                print("Sorry, we didn't find any fast foods satisfy your requirement")

        if requirement == 5:
            calories_limit = input("What is your expected limitation on calories: ")
            for i in range(len(Calories)):
                if str(Calories [i]) <= calories_limit and str(Fats [i]) <= fats_limit and str(Protein[i]) <= protein_limit and str(sodium[i]) <= sodium_limit:
                    filter_list.append(Fastfood[i])
            if len(filter_list) > 0:
                print("Searching...")
                time.sleep(2)
                print(filter_list)
                print("These might be some good choices for you!")
            else: 
                print("Sorry, we didn't find any fast foods satisfy your requirement")
    else: 
        print("Please contact us when you do!")

def finding_location():
    filter_list.clear()
    print("Having trouble with finding your food? Enter the name of the food and we will help you to get your food!")
    yon = input("Do you need some assistance to find your meal: ")
    if yon == "yes" or yon == "y":
        vender_finding = input("What are you looking for (Please just provide us the type of food): ").capitalize()
        for i in range(len(Fastfood)):
            if vender_finding in Fastfood [i]:
                filter_list.append(Vender[i])
        if len(filter_list) > 0: 
            print("Your targeted food could be found at...")
            time.sleep(2)
            print(filter_list)
        elif len(filter_list) > 0 and "Other" in filter_list:
            print("There is no specific vender for your food")
        else: 
            print("Sorry, we don't have related information about your food in our datasets")
            
def fastfoodrecommender():
    print("Welcome to fast food recommender")
    print("We are here to help you to find the nutritions for your interested fast food and where to get the food")
    while True:
        print("Please choose an activity: ")
        print("""1. Check food's nutrition
2. Find foods that satisfy your expectation
3. Find the location that can help you to get your food
4. Exit""")
        Activity = int(input("Activity (1 ~ 4): "))
        if Activity == 1: 
            information_research()
            time.sleep(1)
            continue1 = input("Do you want to search another food? We are willing to help you: ")
            if continue1 == "no" or continue1 == "n":
                print("Thanks for visiting fast food recommander!")
                break

        if Activity == 2:
            favor_selection()
            time.sleep(1)
            continue1 = input("Do you want to search another food? We are willing to help you: ")
            if continue1 == "no" or continue1 == "n":
                print("Thanks for visiting fast food recommander!")
                break

        if Activity == 3: 
            finding_location()
            time.sleep(1)
            continue1 = input("Do you want to search another food? We are willing to help you: ")
            if continue1 == "no" or continue1 == "n":
                print("Thanks for visiting fast food recommander!") 
                break
            
        if Activity == 4: 
            break

fastfoodrecommender()
